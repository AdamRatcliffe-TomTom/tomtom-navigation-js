 1 file changed, 1 insertion(+)

diff --git a/rollup.config.mjs b/rollup.config.mjs
index d0e4aee..d83edcd 100644
--- a/rollup.config.mjs
+++ b/rollup.config.mjs
@@ -32,6 +32,7 @@ export default {
       extensions: [".js", ".jsx"]
     }),
     commonjs({
